<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cansiz.recyclerexample">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterPage" />
        <activity android:name=".AdminHomePage" />
        <activity android:name=".AddStock" />
        <activity android:name=".UpdateAndDeleteStock" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".AdminLoginPage" />
        <activity android:name=".UserHomePage" />
        <activity android:name=".ProductEntry" />
        <activity android:name=".UpdatePassword" />
        <activity android:name=".ProductOutput" />
        <activity android:name=".ReportPage" />
        <activity android:name=".ReportProductPage" />
        <activity android:name=".ReportProductEntryPage" />
        <activity android:name=".ReportProductOutputPage"></activity>
    </application>

</manifest>